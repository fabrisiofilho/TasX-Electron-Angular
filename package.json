{
  "name": "tas-x-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"set ELECTRON_IS_DEV=0 && electron .\" ",
    "server": "concurrently  \"npm --prefix ./frontend run dev\" \"set ELECTRON_IS_DEV=1 && electron .\"",
    "build": "concurrently \"npm run server-build\" \"npm run react-build\"",
    "react-build": "npm --prefix ./frontend run build",
    "react-start": "npm --prefix ./frontend run dev",
    "server-build": "npm --prefix ./backend run build",
    "server-start": "npm --prefix ./backend run server",
    "electron": "set ELECTRON_IS_DEV=0 && electron ."
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4",
    "concurrently": "^7.4.0",
    "electron-is-dev": "^2.0.0",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular/cli": "~14.1.0",
    "@angular/compiler-cli": "^14.1.0",
    "@types/jasmine": "~4.0.0",
    "electron": "^20.1.4",
    "electron-packager": "^16.0.0",
    "electron-rebuild": "^3.2.9",
    "nodemon": "^2.0.20",
    "jasmine-core": "~4.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.7.2"
  }
}
